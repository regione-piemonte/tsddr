{
  "name": "TSDDR",
  "description": "Progetto contenente i sorgenti dell'applicativo TSDDR",
  "version": "0.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href /tsddr/",
    "build": "ng build",
    "build:local": "npm run build -- --aot --configuration=local --base-href /tsddr/",
    "build_dev": "npm run build -- --aot --configuration=dev --base-href /tsddr/",
    "build_tst-rp-01": "npm run build -- --aot --configuration=test --base-href /tsddr/",
    "build_prod-rp-01": "npm run build -- --prod --aot --base-href /tsddr/",
    "mocks": "node ./mocks/server.js",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint scriva --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:styles:fix": "stylelint --fix ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "prepare-commit-msg-unix": "exec < /dev/tty && git cz --hook || true",
    "prepare-commit-msg-win": "prepare-commit-msg",
    "release:dev": "dotenv release-it -- --disable-metrics --ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/forms": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@asymmetrik/ngx-leaflet": "^8.1.0",
    "@nebular/security": "^7.0.0",
    "@ng-bootstrap/ng-bootstrap": "^9.1.3",
    "@ng-select/ng-select": "^6.1.0",
    "@ngneat/until-destroy": "^8.1.4",
    "@ngx-translate/core": "^13.0.0",
    "@swimlane/ngx-datatable": "^20.0.0",
    "@types/leaflet": "^1.7.8",
    "bootstrap": "^4.6.0",
    "bootstrap-italia": "^1.4.3",
    "date-fns": "^2.22.1",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "leaflet": "^1.7.1",
    "leaflet.control.layers.tree": "^1.1.0",
    "ngx-datatable": "^1.0.3",
    "npm": "^6.14.12",
    "popper.js": "^1.16.1",
    "proj4leaflet": "^1.0.2",
    "reproject": "^1.2.6",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.14",
    "@angular/cli": "^11.0.0",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/localize": "^11.2.14",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/jasmine": "^3.7.7",
    "@types/node": "^12.20.15",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^4.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "glob": "^7.1.7",
    "husky": "^5.2.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "lint-staged": "^10.5.4",
    "mobile-app-distribution": "git+https://github.com/ruddenchaux/mobile-app-distribution.git",
    "morgan": "^1.10.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "release-it": "^14.2.2",
    "rimraf": "2.6.1",
    "rxjs-tslint-rules": "^4.34.7",
    "stylelint": "7.13.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^4.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
